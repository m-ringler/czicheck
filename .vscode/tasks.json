{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "test",
      "type": "shell",
      "command": "dotnet test",
      "presentation": {
        "echo": true,
        "reveal": "always",
        "panel": "shared"
      },
      "group": {
        "kind": "test",
        "isDefault": false
      },
      "problemMatcher": []
    }
    ,
    {
      "label": "cmake configure",
      "type": "shell",
      "command": "cmake .. -DCMAKE_BUILD_TYPE=Release",
      "options": {
        "cwd": "${workspaceFolder}/build"
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "panel": "shared"
      },
      "group": {
        "kind": "build",
        "isDefault": false
      },
      "problemMatcher": []
    }
    ,
    {
      "label": "cmake build",
      "type": "shell",
      "command": "cmake --build . --config Release",
      "options": {
        "cwd": "${workspaceFolder}/build"
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "panel": "shared"
      },
      "group": {
        "kind": "build",
        "isDefault": false
      },
      "problemMatcher": []
    }
  ]
}
